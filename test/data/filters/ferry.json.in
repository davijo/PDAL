{
  "Pipeline": {
    "Readers": [{
      "type": "readers.las",
      "options": {
        "filename": "@CMAKE_SOURCE_DIR@/test/data/las/1.2-with-color.las",
        "spatialreference": "EPSG:2993"
      }
    }],
    "Filters": [{
      "type": "filters.ferry",
      "options": {
        "dimensions": "X=StatePlaneX, Y=StatePlaneY"
      }
    }, {
      "type": "filters.reprojection",
      "options": {
        "out_srs": "EPSG:4326+4326",
        "scale_x": "0.0000001",
        "scale_y": "0.0000001"
      }
    }],
    "Writers": [{
      "type": "writers.las",
      "options": {
        "filename": "@CMAKE_SOURCE_DIR@/test/temp/colorized.las"
      }
    }]
  }
}
